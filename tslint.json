{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": false,
    "no-conditional-assignment": true,
    "no-shadowed-variable": false,
    "no-console": true,
    "object-literal-sort-keys": false,
    "max-line-length": [
      true
    ]
  },
  "jsRules": {
    "max-line-length": [
      true
    ]
  }
}